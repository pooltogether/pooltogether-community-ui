{
  "name": "pooltogether-pool-ui",
  "version": "3.0.1",
  "private": false,
  "scripts": {
    "reinstall": "rm -rf node_modules/ && yarn",
    "clean": "rm -rf .next",
    "dev": "next dev",
    "test": "jest",
    "build": "NODE_OPTIONS=\"--max_old_space_size=16384\" next build && next export && yarn postexport",
    "start": "next start",
    "postexport": "node post-export.js",
    "release": "./scripts/sentry-release.sh",
    "distrelease": "export NEXT_JS_RELEASE_VERSION=$SENTRY_PROJECT:$(sentry-cli releases propose-version) && yarn build && yarn release",
    "format": "yarn prettier --write .",
    "check-format": "yarn prettier --check .",
    "link-hooks": "cd node_modules/react && yarn link && cd ../react-query && yarn link && cd ../react-dom && yarn link && cd ../jotai && yarn link && cd ../.. && yarn link @pooltogether/hooks",
    "unlink-hooks": "cd node_modules/react && yarn unlink && cd ../react-query && yarn unlink && cd ../react-dom && yarn unlink && cd ../jotai && yarn unlink && cd ../.. && yarn unlink @pooltogether/hooks"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{ts,js}"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/lib/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@next/bundle-analyzer": "^10.1.1"
  },
  "dependencies": {
    "@pooltogether/bnc-onboard": "1.34.1-i",
    "@pooltogether/bnc-onboard-hooks": "^1.0.5",
    "@pooltogether/current-pool-data": "3.4.23",
    "@pooltogether/etherplex": "1.1.1",
    "@pooltogether/evm-chains-extended": "^0.4.4",
    "@pooltogether/hooks": "^1.3.2",
    "@pooltogether/pooltogether-contracts": "^3.4.3",
    "@pooltogether/pooltogether-rng-contracts": "^1.1.2",
    "@pooltogether/react-components": "^1.4.1",
    "@pooltogether/utilities": "^0.0.26",
    "@reach/menu-button": "^0.11.2",
    "@sentry/cli": "^1.61.0",
    "@sentry/react": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "autoprefixer": "^9.8.1",
    "axios": "^0.21.1",
    "babel-jest": "^26.3.0",
    "beautiful-react-hooks": "^0.31.0",
    "bn.js": "4.11.0",
    "classnames": "^2.2.6",
    "compare-versions": "^3.6.0",
    "d3-scale": "^3.2.3",
    "d3-shape": "^2.0.0",
    "date-fns": "^2.16.1",
    "eth-revert-reason": "^1.0.3",
    "ethers": "^5.1.4",
    "feather-icons-react": "^0.4.1",
    "framer-motion": "^5.3.3",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "graphql-tag": "^2.12.6",
    "jest": "^26.4.2",
    "jotai": "^0.16.10",
    "js-cookie": "^2.2.1",
    "next": "11.1.3",
    "next-images": "^1.7.0",
    "postcss-easy-import": "^3.0.0",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-countup": "^4.3.3",
    "react-device-detect": "^1.13.1",
    "react-dom": "17.0.2",
    "react-loader-spinner": "^3.1.14",
    "react-query": "^3.16.0",
    "react-toastify": "^6.0.5",
    "react-tooltip": "^4.2.15",
    "request": "2.88.2",
    "rollup": "2.17.1",
    "scheduler": "^0.20.1",
    "tailwindcss": "^1.7.6",
    "webpack": "5.28.0"
  }
}
